    <script>
        $(document).ready(function () {

            $("#btnTag").click(function () {
                $("#btnTag").text("Clicked");
            });
            $("#link").click(function () {
                $("#link").text("Clicked");
            });
            $("input").click(function () {
                $(this).val("Clicked");
            });
            $("input").focus(function () {
                $("input").val("Not Focused");
                $(this).val("Focused");
                $(this).val("Focused");
                $(this).select();
            });
        });
    </script>
